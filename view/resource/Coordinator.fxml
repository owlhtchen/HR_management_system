<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.coordinator.CoordinatorController">
    <children>
        <Label layoutX="45.0" layoutY="29.0" prefHeight="17.0" prefWidth="165.0" text="Coordinator Menu">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Button layoutX="320.0" layoutY="125.0" mnemonicParsing="false" onAction="#createPost" prefHeight="27.0" prefWidth="199.0" text="Create Post" />
        <Button layoutX="320.0" layoutY="178.0" mnemonicParsing="false" onAction="#manageInterviews" prefHeight="27.0" prefWidth="199.0" text="Manage Interviews" />
        <Button layoutX="320.0" layoutY="65.0" mnemonicParsing="false" onAction="#checkPosts" prefHeight="27.0" prefWidth="199.0" text="View Posts" />
        <Button layoutX="314.0" layoutY="320.0" mnemonicParsing="false" onAction="#signOut" prefHeight="17.0" prefWidth="199.0" text="Sign out" />
        <Label fx:id="username" layoutX="45.0" layoutY="92.0" text="Username:" />
        <Label fx:id="company" layoutX="45.0" layoutY="125.0" text="Company:" />
        <Label fx:id="locationLabel" layoutX="45.0" layoutY="157.0" text="Location:" />
      <Button layoutX="320.0" layoutY="250.0" mnemonicParsing="false" onAction="#showMailbox" prefHeight="17.0" prefWidth="199.0" text="Open Mailbox" />
    </children>
</AnchorPane>
